{
    "name": "hitek/slimez",
    "description": "slimest is a very slim fast and secure php framework built for Hitek Financials for creating apis backend services for their frontend applications.",
    "type": "project",
    "license": "License for Hitek Financials as an orgainsation, not to be use without the permission of the organization",
    "autoload": {
        "psr-4": {
            "Hitek\\Slimez\\App\\": "App/",
            "Hitek\\Slimez\\Core\\": "Core/",
            "Hitek\\Slimez\\Configs\\": "Configs/"
        }
    },
    "require": {
        "predis/predis": "^2.1",
        "vlucas/phpdotenv": "^5.5",
        "nesbot/carbon": "^2.66",
        "okta/jwt-verifier": "^1.3",
        "spomky-labs/jose": "^5.0",
        "guzzlehttp/psr7": "^2.5",
        "php-amqplib/php-amqplib": "^3.5",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.2",
        "phpoffice/phpword": "^1.1",
        "tecnickcom/tcpdf": "^6.6",
        "amphp/byte-stream": "^2.0",
        "amphp/socket": "^2.1",
        "react/http": "^1.9",
        "react/socket": "^1.13",
        "firebase/php-jwt": "^6.8",
        "paragonie/sodium_compat": "^1.20",
        "php-open-source-saver/jwt-auth": "^2.1",
        "tymon/jwt-auth": "^2.0",
        "symfony/console": "^6.3",
        "symfony/mailer": "^6.3",
        "phpmailer/phpmailer": "^6.8",
        "twilio/sdk": "^7.14"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
